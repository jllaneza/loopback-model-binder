{
  "mongoSpotifyRest": {
    "connector": "rest",
    "debug": "true",
    "options": {
      "headers": {
        "content-type": "application/json"
      },
      "strictSSL": false
    },
    "operations": [
      { 
        "template": {
          "method": "GET",
          "url": "https://api.spotify.com/v1/artists?ids={ids}"
        },
        "functions": {
          "getSpotifySeveralArtists": ["ids"]
        }
      }
    ]
  }
}